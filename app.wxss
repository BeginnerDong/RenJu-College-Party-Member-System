/***********navbar***********/
.navbar{
	height:110rpx;
	width:100%;
	position:fixed;
	bottom:0rpx;
	z-index:999;
	background:#fff;
	display:flex;
	justify-content:center;
	border-top:solid 1px #dbdbdb;
	align-items:center;
	box-shadow:5rpx 0 15rpx #e7b5b2;
}
.nav_img image{
	width:46rpx;
	height:46rpx;
}
.navbar_item{
	width:33.3%;
	text-align:center;
}
.navbar .text{
	color:#999;
	font-size:26rpx;
}
.navbar .this-text{
	color:#b02923;
}
.navigator-hover {
	background:none;
	color:#b02923;
}
.submit_info{
	margin:0rpx auto;
	width:700rpx;
	height:88rpx;
	line-height:88rpx;
	text-align:center;
	border-radius:50rpx;
	color:#fff;
	background:#b02923;
}
.space_bottom{
	width:100%;
	height:120rpx;
}

